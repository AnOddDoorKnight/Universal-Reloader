class HDRel_RawLead : HDRel_CraftingMaterial 
{
	Default
	{
		Tag "$TAG_RAWLEAD";
		Inventory.Icon "LEADA0";
		Inventory.PickupMessage "$PICKUP_RAWLEAD";
		HDRel_CraftingMaterial.PileMessage "$PICKUP_PILE_RAWLEAD";
		HDRel_CraftingMaterial.PileType PType_Ball;
		HDPickup.Bulk 0.25;
		Scale 0.8;
	}

	States
	{
		Spawn:
			LEAD A -1;
			Stop;
	}
}
